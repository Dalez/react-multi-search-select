    ],
    "collectCoverage": true,
    "coverageReporters": [
      "text-summary",
      "lcov",
      "html",
      "text"
    ]
  },
  "keywords": [
    "react",
    "dropdown",
    "multi select",
    "multi search",
    "multi search select",
    "dropdown search",
    "react-multi-search-select",
    "react multi search select",
    "react multisearch select",
    "react multiselect",
    "react multi select"
  ]
