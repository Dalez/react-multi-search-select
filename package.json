{
  "name": "react-multi-search-select",
  "version": "1.2.2",
  "main": "build/index.js",
  "author": "Dale Rushin <support@dalerushin.co.uk>",
  "description": "A fully tested Typescript React component that shows a multi select searchable dropdown",
  "repository": {
    "type": "git",
    "url": "https://github.com/Dalez/react-multi-search-select"
  },
  "license": "MIT",
  "scripts": {
    "start": "parcel index.html",
    "build": "webpack",
    "test": "jest --coverage"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11",
    "@testing-library/react": "^12.1",
    "@testing-library/user-event": "^13.5",
    "@types/jest": "^27.0",
    "@types/react": "^17.0",
    "@types/react-dom": "^17.0",
    "babel-core": "^6.26",
    "babel-preset-env": "^1.7",
    "babel-preset-react": "^6.24",
    "babel-preset-stage-0": "^6.24",
    "css-loader": "^6.5",
    "identity-obj-proxy": "^3.0",
    "jest": "^27.3",
    "react": "^17.0",
    "react-dom": "^17.0",
    "style-loader": "^3.3",
    "ts-jest": "^27.0",
    "ts-loader": "^9.2",
    "typescript": "^4.5",
    "webpack": "^5.64",
    "webpack-cli": "^4.9"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      ".+\\.(css)$": "identity-obj-proxy"
    },
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "text-summary",
      "lcov",
      "html",
      "text"
    ]
  },
  "keywords": [
    "react",
    "dropdown",
    "multi select",
    "multi search",
    "multi search select",
    "dropdown search",
    "react-multi-search-select",
    "react multi search select",
    "react multisearch select",
    "react multiselect",
    "react multi select"
  ]
}
