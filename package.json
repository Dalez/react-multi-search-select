{
  "name": "react-multi-search-select",
  "version": "1.4.0",
  "source": "src/index.tsx",
  "main": "dist/index.js",
  "module": "dist/module.js",
  "types": "dist/types.d.ts",
  "author": "Dale Rushin <support@dalerushin.co.uk>",
  "description": "A fully tested Typescript React component that shows a multi select searchable dropdown",
  "repository": {
    "type": "git",
    "url": "https://github.com/Dalez/react-multi-search-select"
  },
  "license": "MIT",
  "scripts": {
    "start": "parcel index.html",
    "build": "parcel build",
    "test": "jest --coverage"
  },
  "files": [
    "dist/index.css",
    "dist/module.js",
    "dist/types.d.ts"
  ],
  "devDependencies": {
    "@parcel/packager-ts": "^2.0.1",
    "@parcel/transformer-typescript-types": "^2.0.1",
    "@testing-library/jest-dom": "^5.11",
    "@testing-library/react": "^12.1",
    "@testing-library/user-event": "^13.5",
    "@types/jest": "^27.0",
    "@types/react": "^17.0",
    "@types/react-dom": "^17.0",
    "identity-obj-proxy": "^3.0",
    "jest": "^27.3",
    "parcel": "^2.0.1",
    "react": "^17.0",
    "react-dom": "^17.0",
    "ts-jest": "^27.0",
    "typescript": "^4.5"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      ".+\\.(css)$": "identity-obj-proxy"
    },
    "setupFilesAfterEnv": [
